function VisitBlockNode(this: ZSourceEngine, Node: ZBlockNode): void {
	var i: int = 1;
	while(i < Node.GetListSize() && this.IsVisitable()) {
		var StmtNode: ZNode = Node.GetListAt(i);
		this.Eval(StmtNode);
		if(StmtNode.IsBreakingBlock()) {
			break;
		}
	}
}

